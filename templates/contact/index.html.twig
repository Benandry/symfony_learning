{% extends "base.html.twig" %}

{% block body %}

<div class="container mt-5">
    <h1>Contact Us</h1>

    {% for message in app.flashes('success') %}
        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}

    <div class="card">
        <div class="card-body">
           {{ form(contactForm) }}
        </div>
    </div>
</div>

 {% endblock %}